class Solution:
    def sortedSquares(self, nums: List[int]) -> List[int]:
        ans = []
        for i in nums:
            ans.append(i*i)
        ans.sort()
        return ans
        
        
 #other solution in one line
 class Solution:
    def sortedSquares(self, nums: List[int]) -> List[int]:
        return (sorted((i*i for i in nums)))
        
